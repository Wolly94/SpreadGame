rescript = 1
rule cp
  command = cp $i $out
rule touch
 command = touch $out
o Cell.cmi : cp ../bs/src/rescriptSpreadGame/Cell.cmi
o Cell.cmj : cp ../bs/src/rescriptSpreadGame/Cell.cmj
o Cell.cmt : cp ../bs/src/rescriptSpreadGame/Cell.cmt
