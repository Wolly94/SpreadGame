o SpreadGame.cmi : cp ../bs/src/rescriptSpreadGame/SpreadGame.cmi
o SpreadGame.cmj : cp ../bs/src/rescriptSpreadGame/SpreadGame.cmj
o SpreadGame.cmt : cp ../bs/src/rescriptSpreadGame/SpreadGame.cmt
o SpreadGame.res : cp ../../src/rescriptSpreadGame/SpreadGame.res
build install.stamp : touch Cell.cmi Cell.cmj SpreadGame.cmi SpreadGame.cmj 
